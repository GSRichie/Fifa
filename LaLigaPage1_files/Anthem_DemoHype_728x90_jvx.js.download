(lib.cta_mc = function(mode,startPosition,loop) {
if (loop == null) { loop = false; }	this.initialize(mode,startPosition,loop,{});

	// timeline functions:
	this.frame_0 = function() {
		function dtpResize(textCopy, textHeightEN, maxHeight, verticalAlign, reSize, ignoreLB) {
		    if (ignoreLB == true) {
		        textCopy.text = textCopy.text.replace("\n", "");
		    }
		    var myHeight = textCopy.getBounds().height;
		    var myWidth = textCopy.getBounds().width;
		    if (reSize == true) {
		        if (myHeight > maxHeight + 1) {
		            textCopy.scaleX = textCopy.scaleY = maxHeight / myHeight;
		            textCopy.lineWidth = textWidthEN * myHeight / maxHeight;
		            var myHeightNew = textCopy.getBounds().height;
		            if (myHeightNew < myHeight) {
		                var myWidthNew = textCopy.getBounds().width;
		                var myWidthNewScaled = myWidthNew * textCopy.scaleX;
		                var myNewScale = textWidthEN / myWidthNewScaled;
		                textCopy.scaleX = textCopy.scaleY = textCopy.scaleX * myNewScale;
		                myHeight = myHeightNew;
		            }
		        }
		    }
		    if (verticalAlign == "top") {
		    }
		    if (verticalAlign == "center") {
		        textCopy.y = textCopy.y + (textHeightEN - myHeight * textCopy.scaleY) / 2;
		    }
		    if (verticalAlign == "bottom") {
		        textCopy.y = textCopy.y + (textHeightEN - myHeight * textCopy.scaleY);
		    }
		}
		
		
			//TEXT TOOL BEGIN 
			var textCopy = this.cta_300x250; //update name of text 
			var reSize = false; 
			var ignoreLB = false; 
			var numRowsEN = 1; // number of rows english 
			var maxRows = 3; //number of max rows (you can also choose e.g. 2.9 if 3 is too  much. Then the text will be scaled)		 
			var verticalAlign = "center"; //center, top, bottom. MUST BE NAMED EXACTLY THIS WAY! 
		
			//dont touch
			var lineHeightEN = textCopy.lineHeight; //dont touch
			var textWidthEN = textCopy.lineWidth;	// dont touch
			var textHeightEN = lineHeightEN * numRowsEN; //  dont touch - EN height
			var maxHeight = lineHeightEN * maxRows; // dont touch - MAX height
			dtpResize(textCopy,textHeightEN,maxHeight,verticalAlign,reSize,ignoreLB);
			//TEXT TOOL END
	}

	// actions tween:
	this.timeline.addTween(cjs.Tween.get(this).call(this.frame_0).wait(1));

	// Layer_2
	this.cta_300x250 = new cjs.Text(lib.jvxStrings[1].text, lib.jvxStrings[1].font, lib.jvxStrings[1].color);
	this.cta_300x250.name = "cta_300x250";
	this.cta_300x250.textAlign = "center";
	this.cta_300x250.lineHeight = lib.jvxStrings[1].lineHeight;
	this.cta_300x250.lineWidth = 119;
	this.cta_300x250.parent = this;
	this.cta_300x250.setTransform(79.45,6.9);
	try { _jvxSetTextVertPos(lib, 1, this.cta_300x250); } catch(ex){}

	this.timeline.addTween(cjs.Tween.get(this.cta_300x250).wait(1));

	// Layer_1
	this.instance = new lib.cta_bg();
	this.instance.parent = this;

	this.timeline.addTween(cjs.Tween.get(this.instance).wait(1));

}).prototype = getMCSymbolPrototype(lib.cta_mc, new cjs.Rectangle(0,0,156,28), null);


